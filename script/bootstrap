#!/bin/bash
ln -sf $HOME/dotfiles/emacs		~/.config/emacs
ln -sf $HOME/dotfiles/terminal		~/.config/terminal
ln -sf $HOME/dotfiles/.bashrc		~/.bashrc
ln -sf $HOME/dotfiles/.xprofile	~/.xprofile
ln -sf $HOME/dotfiles/fancyprompts	~/.config/fancyprompts
ln -sf $HOME/dotfiles/qtile		~/.config/qtile
ln -sf $HOME/dotfiles/systemd		~/.config/systemd
ln -sf $HOME/dotfile/fonts		~/.local/share/fonts
